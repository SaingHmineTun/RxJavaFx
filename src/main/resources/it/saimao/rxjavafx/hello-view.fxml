<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefWidth="300.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.saimao.rxjavafx.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <ListView fx:id="listFx" prefHeight="250.0" prefWidth="200.0" />
   <Button fx:id="btBlocking" maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefHeight="36.0" text="Blocking">
      <font>
         <Font name="Monospaced Bold" size="14.0" />
      </font>
   </Button>
   <Button fx:id="btNonBlocking" maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefHeight="36.0" text="Non-Blocking">
      <font>
         <Font name="Monospaced Bold" size="14.0" />
      </font>
   </Button>
   <Button fx:id="btReactiveX" maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefHeight="36.0" text="Non Blocking Javafx">
      <font>
         <Font name="Monospaced Bold" size="14.0" />
      </font>
   </Button>
   <Button fx:id="btClear" layoutX="30.0" layoutY="436.0" maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefHeight="36.0" text="CLEAR LIST" textFill="#6f0d0d">
      <font>
         <Font name="Monospaced Bold" size="14.0" />
      </font>
   </Button>
</VBox>
